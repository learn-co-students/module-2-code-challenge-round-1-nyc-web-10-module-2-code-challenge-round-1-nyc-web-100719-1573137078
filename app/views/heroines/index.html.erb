<h2>All Heroines</h2>
<p>Filter for a superpower</p>
<%= form_tag heroines_path, method: :get do%>
  <%=label_tag :search, 'Power:'%>
  <%= text_field_tag :search %>
  <%= submit_tag 'Search'%>
<%end%>
<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %>
    </li>
    <br>
  <% end %>
</ul>
<%=link_to 'Make a new heroine', new_heroine_path%>
