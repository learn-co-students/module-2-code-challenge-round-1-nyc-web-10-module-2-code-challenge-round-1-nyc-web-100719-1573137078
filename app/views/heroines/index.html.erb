<h2>All Heroines</h2>
<h3>Search by super power</h3>
<%= form_tag(heroines_path) do %>
  Store <%= text_field_tag :name %>
  <%= submit_tag 'Search' %>
<% end %>
<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %>
    </li>
    
    <br>
  <% end %>
</ul>
