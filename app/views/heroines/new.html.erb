<h1> Create a new heroine! </h1> 

<%= form_for @heroine do |h| %>
    <%= h.label :name %>
    <%= h.text_field :name %><br />
  
    <%= h.label :super_name %>
    <%= h.text_field :super_name %><br />
  
    <%= h.collection_select(:power_id, @powers, :id, :name, prompt: true) %> 
    <%= h.submit %>
  <% end %>