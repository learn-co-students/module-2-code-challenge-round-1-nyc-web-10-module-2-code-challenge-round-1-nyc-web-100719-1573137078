<h1>Create a new heroine...</h1>
<p><% flash.each do |key, value| %>
    <%= key %><%= value %><
<% end %>
</p>
<%= form_for @heroine do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :super_name %><br>
    <%= f.text_field :super_name %><br>
    <%= f.label :power %><br>
    <%= collection_select(:power_id, :heroine_id, @powers, :id, :name, {:prompt => true}) %>
    <%= f.submit %>
<% end %>